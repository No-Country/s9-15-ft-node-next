{
  "private": true,
  "name": "soundwave",
  "version": "0.0.1",
  "scripts": {
    "clean": "turbo run clean --no-daemon",
    "dev": "dotenv -- turbo run dev --no-daemon --concurrency 20",
    "dc": "dotenv -- turbo run dc --no-daemon --concurrency 20",
    "build": "turbo run build --no-daemon",
    "check": "turbo run check --no-daemon",
    "storybook:dev": "turbo run storybook:dev --no-daemon",
    "storybook:build": "turbo run storybook:build --no-daemon",
    "test:e2e": "turbo run test:e2e --concurrency=1 --no-daemon",
    "test:unit": "turbo run test:unit --concurrency=1 --no-daemon",
    "lint:prettier": "prettier --plugin-search-dir=. --check .",
    "lint:eslint": "eslint --cache .",
    "lint": "run-s lint:*",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --plugin-search-dir=. --write .",
    "fix": "run-s fix:*",
    "prepare": "husky install",
    "lint-error": " prettier --write  . && eslint . --fix"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@playwright/test": "1.35.1",
    "@storybook/blocks": "7.0.24",
    "@storybook/jest": "0.1.0",
    "@storybook/react": "7.0.24",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@types/testing-library__jest-dom": "5.14.6",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.43.0",
    "eslint-config-custom": "workspace:*",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "storybook": "7.0.24",
    "turbo": "1.10.6",
    "typescript": "5.1.6"
  },
  "packageManager": "pnpm@8.6.5",
  "dependencies": {
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.1",
    "cloudinary": "^1.37.3",
    "flowbite": "^1.7.0",
    "flowbite-react": "^0.4.10",
    "multer": "1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "next-auth": "^4.22.1",
    "react-icons": "^4.10.1"
  }
}
